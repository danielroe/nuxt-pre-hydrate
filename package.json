{
  "name": "nuxt-pre-hydrate",
  "version": "0.0.1",
  "license": "MIT",
  "description": "Safely run pre-hydration steps on the client with Nuxt 3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielroe/nuxt-pre-hydrate.git"
  },
  "keywords": [
    "nuxt",
    "module",
    "nuxt-module",
    "hydration"
  ],
  "author": {
    "name": "Daniel Roe <daniel@roe.dev>",
    "url": "https://github.com/danielroe"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm dev:prepare && nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "pnpm nuxt-module-build build --stub && nuxi prepare playground",
    "docs:dev": "nuxi dev docs",
    "docs:build": "nuxi generate docs",
    "lint": "pnpm lint:all:eslint",
    "lint:all:eslint": "pnpm lint:eslint .",
    "lint:eslint": "eslint --fix",
    "prepack": "pnpm build",
    "prepare": "husky install",
    "prepublishOnly": "pnpm lint && pnpm test && pinst --disable",
    "release": "bumpp && npm publish",
    "test": "vitest run",
    "test:types": "tsc --noEmit",
    "postinstall": "husky install",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "@nuxt/kit": "^3.11.2"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "0.3.13",
    "@nuxt/module-builder": "0.8.3",
    "@nuxt/schema": "3.13.0",
    "@nuxt/test-utils": "3.14.1",
    "@types/node": "20.12.7",
    "@vitest/coverage-v8": "2.0.5",
    "bumpp": "9.4.2",
    "eslint": "9.7.0",
    "expect-type": "0.19.0",
    "get-port-please": "3.1.2",
    "husky": "9.0.11",
    "lint-staged": "15.2.9",
    "magic-regexp": "0.8.0",
    "nuxt": "3.13.0",
    "pinst": "3.0.0",
    "playwright": "1.45.3",
    "typescript": "5.3.3",
    "vite": "5.3.4",
    "vitest": "2.0.5",
    "vue": "3.4.38"
  },
  "resolutions": {
    "nuxt-pre-hydrate": "link:."
  },
  "packageManager": "pnpm@9.8.0",
  "volta": {
    "node": "20.12.2"
  }
}
